	@list='$(header_HEADERS)'; test -n "$(headerdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(headerdir)'; $(am__uninstall_files_from_dir)

